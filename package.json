{
  "name": "thebyteplay-deepagent",
  "version": "1.0.0",
  "description": "TheBytePlay - Full-stack gaming platform with esports content, live streaming, creator profiles, and tournaments. Generated by DeepAgent.",
  "main": "index.js",
  "scripts": {
    "dev": "cd app && yarn dev",
    "build": "cd app && yarn build",
    "start": "cd app && yarn start",
    "test": "cd app && yarn test",
    "install-deps": "cd app && yarn install",
    "setup": "cd app && yarn install && yarn prisma generate && yarn prisma db push",
    "db:seed": "cd app && yarn prisma db seed",
    "db:reset": "cd app && yarn prisma db push --force-reset && yarn prisma db seed"
  },
  "keywords": [
    "gaming",
    "esports",
    "nextjs",
    "typescript",
    "tailwind",
    "prisma",
    "gaming-platform",
    "streaming",
    "tournaments",
    "community"
  ],
  "author": {
    "name": "DeepAgent AI",
    "email": "support@deepagent.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iammunzir/thebyteplay-deepagent.git"
  },
  "bugs": {
    "url": "https://github.com/iammunzir/thebyteplay-deepagent/issues"
  },
  "homepage": "https://github.com/iammunzir/thebyteplay-deepagent#readme",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "workspaces": [
    "app"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
